<Window x:Class="Practica10Ejercicio4.InputInicioSesion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practica10Ejercicio4"
        mc:Ignorable="d"
        Title="InputInicioSesion" Height="450" Width="800">
    <Grid>
        <Label Content="Contaseña:" HorizontalAlignment="Left" Margin="50,50,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtpassword" HorizontalAlignment="Left" Height="23" Margin="150,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="200"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="140,80,0,0" VerticalAlignment="Top">
            <Button x:Name="btnAceptar" Content="Aceptar" Width="100" Margin="5" Click="btnAceptar_Click"/>
            <Button x:Name="btnCancelar" Content="Cancelar" Width="100" Margin="5" Click="btnCancelar_Click"/>
        </StackPanel>

    </Grid>
</Window>
